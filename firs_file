#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent { label 'master' }
    stages {
        stage("First") {
            steps {
                echo "Hello, World"
            }
        }
        stage("Second") {
            steps {
                echo "Hello, N.Novgorod"
            }
        }
        stage("Three") {
            steps {
                script{
                     ssh root@10.122.0.195 \'uptime\'
                }
            }
        }
    }
}
