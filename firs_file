pipeline {
    agent  { 
        label 'master'
           }
  stages {
        stage("First step") {
            steps {
                echo "Hello  WorlD"
                   }
                             }
         stage("Second step") {
            steps {
                sshagent {
                     sh 'sshpass -p \'q1\' ssh root@10.122.0.195 "ls; hostname; whois google.com;"'
                  }
                  }
               }
        
    }
}
