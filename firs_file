pipeline {
    agent { label 'master' }
    stages {
        stage('First') {
            steps {
                echo "Hello, World"
            }
        }
        stage('Second') {
            steps {
                ping 10.122.0.195
            }
        }
        stage('Three') {
            steps {
                sh 'ssh root@10.122.0.195 \'uptime\''
            }
        }
    }
}
